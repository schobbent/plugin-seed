{
  "name": "pouchdb-plugin-seed",
  "version": "0.1.0",
  "description": "PouchDB Plugin Seed project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pouchdb/plugin-seed.git"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "plugin",
    "seed",
    "couch",
    "couchdb"
  ],
  "author": "",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/pouchdb/plugin-seed/issues"
  },
  "scripts": {
    "test-node": "set TEST_DB=testdb,http://localhost:5984/testdb && istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "test-browser": "./bin/test-browser.js",
    "jshint": "gulp jshint",
    "test": "npm run jshint && sh bin/run-test.sh",
    "build": "mkdirp dist && mkdirp temp-browserify && npm run browserify && npm run gulp",
    "browserify": "browserify index.js | derequire > temp-browserify/index.js",
    "gulp": "gulp afterbrowserify",
    "dev": "browserify test/test.js > test/test-bundle.js && npm run dev-server",
    "dev-server": "./bin/dev-server.js",
    "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
  },
  "dependencies": {
    "lie": "^2.6.0",
    "inherits": "~2.0.1",
    "argsarray": "0.0.1"
  },
  "devDependencies": {
    "bluebird": "^1.0.7",
    "browserify": "^9.0.8",
    "chai": "^1.9.1",
    "derequire": "^2.0.0",
    "es3ify": "^0.1.3",
    "gulp": "^3.9.1",
    "gulp-es3ify": "0.0.0",
    "gulp-jshint": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "http-server": "~0.5.5",
    "inherits": "^2.0.1",
    "istanbul": "^0.2.7",
    "jshint": "^2.9.2",
    "mkdirp": "^0.5.0",
    "mocha": "^1.18.2",
    "phantomjs": "^1.9.7-5",
    "pouchdb": "pouchdb/pouchdb",
    "request": "^2.36.0",
    "sauce-connect-launcher": "^0.4.2",
    "selenium-standalone": "^3.0.2",
    "uglify-js": "^2.4.13",
    "watchify": "^3.1.0",
    "wd": "^0.2.21"
  }
}
